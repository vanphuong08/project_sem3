        @Html.Partial("~/Areas/Admin/Views/Shared/Header.cshtml");
        <!-- /.navbar -->
        <!-- Main Sidebar Container -->
        @Html.Partial("~/Areas/Admin/Views/Shared/SideBar.cshtml");

